{
  "password_length": {
    "doc": "Password length",
    "default": 10,
    "env": "PASSWORD_LENGTH",
    "arg": "password_length"
  },
  "secret_key": {
    "doc": "JWT Secret Key",
    "default": "@^&12^(jhfg(^HG(0_",
    "env": "SECRET_KEY",
    "arg": "secret_key"
  },
  "token_expiry": {
    "doc": "Token expiry in miliseconds (48 hours by default)",
    "default": 172800000,
    "env": "TOKEN_EXPIRY",
    "arg": "token_expiry"
  }
}
